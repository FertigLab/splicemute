
        START=1
        END=117
        for ((VAR=$START ; VAR<=$END; VAR++));
        do
            STAR_JUNCFILE=$(sed -n ${VAR}p /dcs04/fertig/data/theron/share/bams/SJ_OUT/SJ_files.txt)
            /users/tpalmer/splicemute/scripts/filter_juncs.R -o /dcs04/fertig/data/theron/splicemutr_snakemake_example/leafcutter_output/filtered_star_files -s $STAR_JUNCFILE
        done
        
        ls /dcs04/fertig/data/theron/splicemutr_snakemake_example/leafcutter_output/filtered_star_files/*.filt > /dcs04/fertig/data/theron/splicemutr_snakemake_example/leafcutter_output/filtered_star_files/filt_files.txt
        