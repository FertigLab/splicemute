
    START=1
    END=114
    for ((VAR=$START ; VAR<=$END; VAR++));
    do
      STAR_JUNCFILE=$(sed -n ${VAR}p /dcs04/fertig/data/theron/splicemutr_snakemake_example/leafcutter_output/filtered_star_files/filt_files.txt)
      OUT_DIR=$(dirname /dcs04/fertig/data/theron/splicemutr_snakemake_example/leafcutter_output/juncs/filenames.txt)
      /users/tpalmer/splicemute/scripts/STAR_to_leaf.R -f /users/tpalmer/splicemute/R/functions.R -o $OUT_DIR -s $STAR_JUNCFILE
    done

    cd $OUT_DIR
    ls $PWD/*.junc > filenames.txt
    