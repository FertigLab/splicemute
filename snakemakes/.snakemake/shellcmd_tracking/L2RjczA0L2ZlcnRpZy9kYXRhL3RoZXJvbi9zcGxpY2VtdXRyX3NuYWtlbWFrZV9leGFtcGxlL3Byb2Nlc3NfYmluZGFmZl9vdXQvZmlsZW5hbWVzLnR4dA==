
        START=1
        for ((VAR=$START; VAR<=63; VAR++))
        do
            ALLELE=$(sed -n ${VAR}p /dcs04/fertig/data/theron/splicemutr_snakemake_example/mhcnuggets_out/allele_files.txt)
            BINDERS=/dcs04/fertig/data/theron/splicemutr_snakemake_example/process_bindaff_out/$(echo $(basename $ALLELE) | sed 's/.txt/_filt.txt/g')
            awk -F "," '{ if ($2 <= 500) { print } }' $ALLELE > $BINDERS

            /users/tpalmer/splicemute/inst/process_bindaff.py -b $BINDERS -p /dcs04/fertig/data/theron/splicemutr_snakemake_example/process_peptides_out -o /dcs04/fertig/data/theron/splicemutr_snakemake_example/process_bindaff_out -k 9
        done

        cd /dcs04/fertig/data/theron/splicemutr_snakemake_example/process_bindaff_out
        ls $PWD/*filt.txt > filenames.txt

        